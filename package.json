{
  "name": "string-mismatch",
  "version": "0.2.4",
  "description": "All mismatch between two strings",
  "main": "src/string-mismatch.js",
  "scripts": {
    "bundle-report": "webpack-bundle-analyzer dist/stats.json -h localhost -p 8085",
    "build": "node ./scripts/build && node ./node_modules/webpack/bin/webpack --config ./webpack.config.js --json > dist/stats.json && npm run bundle-report",
    "build:prod": "node ./scripts/build && node ./node_modules/webpack/bin/webpack --mode=\"production\" --config ./webpack.config.js",
    "lint": "eslint -c .eslintrc src/**/*.js",
    "perf": "node ./scripts/performance-test",
    "start": "npm run build:prod && node ./example/mismatch",
    "test": "mocha --ui bdd --recursive ./src",
    "test:cov": "node node_modules/nyc/bin/nyc --reporter=lcov mocha --ui bdd --recursive ./src",
    "pub": "npm run build && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wil92/string-mismatch.git"
  },
  "keywords": [
    "string"
  ],
  "author": {
    "name": "wil92",
    "email": "ggjnez92@gmail.com",
    "url": "https://github.com/wil92"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/wil92/string-mismatch/issues"
  },
  "homepage": "https://github.com/wil92/string-mismatch#readme",
  "dependencies": {
    "lodash": "^4.17.5"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^5.15.0",
    "mocha": "^5.0.5",
    "nyc": "^13.3.0",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.29.5",
    "webpack-bundle-analyzer": "^3.1.0",
    "webpack-cli": "^3.2.3"
  }
}
